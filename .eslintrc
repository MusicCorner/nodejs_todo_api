{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": "airbnb-base",
	"rules": {
		"no-console": 0,
		"radix": 0,
		"indent": [
			2,
			2,
			{
				"SwitchCase": 1
			}
		],
		"callback-return": 0,
		"consistent-return": 0,
		"array-callback-return": 0,
		"no-tabs": 0,
		"arrow-parens": 0,
		"arrow-body-style": [
			"warn",
			"as-needed"
		],
		"no-confusing-arrow": 0,
		"implicit-arrow-linebreak": 0,
		"object-curly-newline": [
			"error",
			{
				"ImportDeclaration": "never",
				"ExportDeclaration": {
					"multiline": true,
					"minProperties": 3
				}
			}
		]
	}
}